<%= render 'layouts/show_header' %>

<%= render 'layouts/flash_success' %>

<div class="row">

  <div class="col-sm-12">

    <div class="well">

      <div>
        <strong>Number:</strong>
        <%= @address.number %>
      </div>

      <div>
        <strong>Complement:</strong>
        <%= @address.complement %>
      </div>

      <div>
        <strong>Cep:</strong>
        <%= @address.cep %>
      </div>

    </div>

  </div>

</div>

<div class="row">
  <div class="col-sm-12">
    <%= link_to 'Edit', edit_address_path(@address), class: 'btn btn-primary btn-xs' %>
    <%= link_to 'Back', street_addresses_path(@address.street), class: 'btn btn-warning btn-xs' %>
  </div>
</div>